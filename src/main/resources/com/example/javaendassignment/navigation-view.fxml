<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox fx:id="layout" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
<VBox alignment="TOP_CENTER" prefHeight="284.0" prefWidth="150.0" spacing="10.0">
   <children>
      <Button fx:id="dashboardButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onDashButtonClick" text="Dashboard" />
      <Button fx:id="createOrderButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onCreateOrderButtonClick" text="Create order" />
      <Button fx:id="productInventoryButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onProductInventoryButtonClick" text="Product inventory" />
      <Button fx:id="orderHistoryButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onOrderHistoryButtonClick" text="Order history" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
</HBox>
